</Line>
<Line Number="47">
<![CDATA[]]>
</Line>
<Line Number="48">
<![CDATA[#region Prevent overflow]]>
</Line>
<Line Number="49">
<![CDATA[	if Snake.Length>30 then]]>
</Line>
<Line Number="50">
<![CDATA[		Snake.Length	:=0;			//:)]]>
</Line>
<Line Number="51">
<![CDATA[		GameOver		:=TRUE;			//Stop game.]]>
</Line>
<Line Number="52">
<![CDATA[		HMI_StartGame	:=FALSE;		//Stop game.]]>
</Line>
<Line Number="53">
<![CDATA[	end_if;]]>
</Line>
<Line Number="54">
<![CDATA[#endregion]]>
</Line>
<Line Number="55">
<![CDATA[]]>
</Line>
<Line Number="56">
<![CDATA[#region High score]]>
</Line>
<Line Number="57">
<![CDATA[	if GameOver and NOT HMI_StartGame and wrk_CheckScoreBoard then]]>
</Line>
<Line Number="58">
<![CDATA[		/* ]]>
</Line>
<Line Number="59">
<![CDATA[			check if current score is higher then any highscore following 1st 2nd and then 3rd. ]]>
</Line>
<Line Number="60">
<![CDATA[							Once true add highscore to highscore array. ]]>
</Line>
<Line Number="61">
<![CDATA[		*/	]]>
</Line>
<Line Number="62">
<![CDATA[		for i := 0 to 2 do]]>
</Line>
<Line Number="63">
<![CDATA[			if Snake.Length>Snake.HighScore.Score[i]then]]>
</Line>
<Line Number="64">
<![CDATA[				/* shift high score first before adding new high score. */]]>
</Line>
<Line Number="65">
<![CDATA[				for ii := i to 2 do]]>
</Line>
<Line Number="66">
<![CDATA[					if 1 >= i then]]>
</Line>
<Line Number="67">
<![CDATA[						Snake.HighScore.Score[i+1]:=Snake.HighScore.Score[i];]]>
</Line>
<Line Number="68">
<![CDATA[						Snake.HighScore.Names[i+1]:=Snake.HighScore.Names[i];]]>
</Line>
<Line Number="69">
<![CDATA[					end_if;]]>
</Line>
<Line Number="70">
<![CDATA[				end_for;]]>
</Line>
<Line Number="71">
<![CDATA[				Snake.HighScore.Score[i]:=Snake.Length;		// add new high score.]]>
</Line>
<Line Number="72">
<![CDATA[				wrk_CheckScoreBoard:=FALSE;			// add score once.]]>
</Line>
<Line Number="73">
<![CDATA[				exit;// cancel forloop]]>
</Line>
<Line Number="74">
<![CDATA[			end_if;]]>
</Line>
<Line Number="75">
<![CDATA[		end_for;]]>
</Line>
<Line Number="76">
<![CDATA[	end_if;]]>
</Line>
<Line Number="77">
<![CDATA[]]>
</Line>
<Line Number="78">
<![CDATA[#endregion]]>
</Line>
<Line Number="79">
<![CDATA[]]>
</Line>
<Line Number="80">
<![CDATA[#region New Game ]]>
</Line>
<Line Number="81">
<![CDATA[	if s:fs OR HMI_NewGame then]]>
</Line>
<Line Number="82">
<![CDATA[		for i := 0 to 31 do				// reset snake]]>
</Line>
<Line Number="83">
<![CDATA[			Snake.X[i]:=0;				// clear previous values]]>
</Line>
<Line Number="84">
<![CDATA[			Snake.Y[i]:=0;				// clear previous values]]>
</Line>
<Line Number="85">
<![CDATA[		end_for;]]>
</Line>
<Line Number="86">
<![CDATA[		/* Snake start position */]]>
</Line>
<Line Number="87">
<![CDATA[		HMI_GameSpeed		:=1000;			// predefined snake speed.]]>
</Line>
<Line Number="88">
<![CDATA[		HMI_Score			:=0;			// fuck you, new game so score is 0. ]]>
</Line>
